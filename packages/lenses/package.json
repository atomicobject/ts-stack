{
  "name": "@atomic-object/lenses",
  "version": "0.1.5",
  "description": "A lens library for TypeScript",
  "repository": "https://github.com/atomicobject/ts-stack",
  "author": "Atomic Object",
  "license": "MIT",
  "typeScriptVersion": "2.3",
  "scripts": {
    "build": "yarn build:es && yarn build:commonjs",
    "build:es": "tsc",
    "build:commonjs": "tsc -p ./tsconfig.commonjs.json",
    "watch": "nodemon -w src -x 'yarn build'",
    "clean": "rimraf es lib",
    "prepublish": "yarn build"
  },
  "sideEffects": false,
  "main": "commonjs/index.js",
  "module": "es/index.js",
  "typings": "es/index",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "commonjs",
    "es"
  ],
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/lodash-es": "^4.17.0",
    "jest": "^20.0.4",
    "jsverify": "^0.8.2",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.4",
    "nodemon": "^1.14.10",
    "rimraf": "^2.6.2",
    "ts-jest": "^20.0.11",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@atomic-object/records": "^0.1.0"
  }
}
