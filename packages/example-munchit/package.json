{
  "name": "munchit",
  "private": true,
  "version": "1.0.2",
  "liense": "MIT",
  "browserslist": ["last 2 versions", "IE 11"],
  "scripts": {
    "start": "nf start",
    "build:graphql": "cli build:graphql",
    "build:client": "webpack --config ./webpack/client.config.js",
    "build:server": "webpack --config ./webpack/server.config.js",
    "build": "cli build",
    "build:production": "cross-env NODE_ENV=production cli build",
    "dev": "cli dev",
    "dev:storybook": "start-storybook -p 9001 -c .storybook",
    "lint":
      "tslint -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "lint:fix":
      "tslint --fix -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "fmt:prettier": "prettier --write 'modules/**/*.{ts,tsx,js,jsx}'",
    "fmt": "yarn fmt:prettier; yarn lint:fix",
    "pretest": "yarn lint",
    "test": "yarn test:unit && yarn test:acceptance",
    "test:truncate": "NODE_ENV=test node dist/scripts/truncate-database.js",
    "test:unit":
      "cross-env NODE_ENV=test jest --forceExit --config=../../jest.config.js",
    "test:unit:debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "test:acceptance":
      "if [ $BUILD ]; then yarn test:acceptance:build; fi; concurrently --kill-others -s first 'yarn test:acceptance:server' 'yarn test:acceptance:run'",
    "test:acceptance:build-and-run":
      "yarn test:acceptance:build && yarn test:acceptance",
    "test:acceptance:build": "cross-env NODE_ENV=test yarn build",
    "test:acceptance:server":
      "NODE_ENV=test node dist/server.js > tests/acceptance/last-run.log",
    "test:acceptance:run":
      "while ! nc -z localhost 3002; do sleep 0.1; done; cross-env NODE_ENV=test jest -w1 -c ./acceptance.jestrc.json",
    "docker:up": "exec docker-compose up",
    "db:create": "yarn db:create:dev; yarn db:create:test",
    "db:create:dev":
      "docker-compose exec  postgres createdb -h postgres development",
    "db:create:test": "docker-compose exec  postgres createdb -h postgres test",
    "db:run": "docker-compose exec  postgres psql -h postgres",
    "db:migrate:make": "knex migrate:make",
    "db:migrate:latest": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:seed:make": "knex seed:make",
    "db:seed:run": "knex seed:run"
  },
  "pre-commit": [],
  "resolutions": {
